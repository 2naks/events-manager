{
  "name": "events-manager",
  "version": "1.0.0",
  "description": "Application for managing event centers. This app will help you accept applications to use your center/facilities, and will either decline events when the proposed day is already taken, or suggest an available day",
  "main": "./server/server.js",
  "scripts": {
    "start": "node ./build/server.js",
    "build-server": " node ./node_modules/babel-cli/bin/babel.js -d ./build ./server",
    "heroku-postbuild": "npm run build-server && npm run migrate && npm run seed",
    "migrate": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate",
    "seed": "node ./node_modules/sequelize-cli/lib/sequelize db:seed:all",
    "start-dev": "nodemon --exec node ./node_modules/babel-cli/bin/babel-node.js ./server/server.js",
    "test": "nyc mocha  --compilers js:babel-core/register ./server/test --timeout 60000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emmabaye/events-manager.git"
  },
  "author": "Emmanuel Abaye",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emmabaye/events-manager/issues"
  },
  "homepage": "https://github.com/emmabaye/events-manager#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "node-sass": "^4.7.2",
    "nyc": "^11.2.1",
    "prettier-eslint-cli": "^4.4.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "sequelize": "^4.22.8",
    "sequelize-cli": "^3.1.0",
    "validator": "^9.1.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
